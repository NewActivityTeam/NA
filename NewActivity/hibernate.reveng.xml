	<table name="tb_comment" catalog="na"
		class="com.na.entity.Comment">
	</table>
	<table name="tb_image" catalog="na" class="com.na.entity.Image"></table>
	<table name="tb_participation" catalog="na"
		class="com.na.entity.PCP">
	</table>
	<table name="tb_userinfo" catalog="na"
		class="com.na.entity.Userinfo">
	</table>
	<table name="tb_vote_detail" catalog="na"
		class="com.na.entity.VoteDetail">
	</table>
	<table name="tb_vote_item" catalog="na"
		class="com.na.entity.VoteItem">
	</table>
	<table name="tb_vote" catalog="na" class="com.na.entity.Vote"></table>
</hibernate-reverse-engineering>